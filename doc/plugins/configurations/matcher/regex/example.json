{
    "rules": [
        {
            "fields": ["purpose"],
            "pattern": "/R(?P<extid>[1-9][0-9]{1,5})K(?P<campaign>[1-9][0-9]{1,3})L[0-9]{1,2}/",
            "comment": "Format of scanned code is RXXXXXXKXXXXLXX, number values, no leading zeroes",
            "actions": [
                {
                    "from": "extid",
                    "action": "copy",
                    "to": "external_identifier"
                },
                {
                    "from": "campaign",
                    "action": "copy",
                    "to": "campaign_id"
                }
            ]
        }
    ]
}
